type Query{
    user: User
}
type Mutation {
    addUser(user: UserInput): User
}

input UserInput {
    nickname: String!
    email: String!
    gender: String!
    age: Int!
    address: String!
    social: String!
}

type User {
    id: ID
    nickname: String!
    email: String!
    gender: String!
    age: Int!
    address: String!
}
