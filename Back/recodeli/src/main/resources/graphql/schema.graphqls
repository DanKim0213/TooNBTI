type Query{
    userById(id: ID!): User
}

type Mutation{
#    addUser(UserSocialRegistParamDTO: UserInput): User
    addUser(user: UserInput): User
}

input UserInput {
    nickname: String!
    email: String!
    gender: String!
    age: String!
    social: String!
}

type User {
    userId: ID
    nickName: String
    email: String
    gender: String
    userAge: Int
    social: String
    roleType: String
    password: String
}
